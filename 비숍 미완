#include <iostream>

using namespace std;

int n;

int a[11][11];
int check[2][21];

void output(){
    
}

int back_tracking(int step){
    if(step>n*n-1){
        
        return 1;
    }
    
}

int main()
{
    int i,j;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        for(j=0;j<n;j++){
            scanf("%d",&a[i][j]);
            if(a[i][j]){
                check[0][n+i-j]=1;
                check[1][i+j]
            }
        }
    }
    
    
    
    
    return 0;
}
